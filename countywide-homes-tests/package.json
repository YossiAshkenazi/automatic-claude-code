{
  "name": "countywide-homes-e2e-tests",
  "version": "1.0.0",
  "description": "Comprehensive E2E testing suite for Countywide Homes property management platform",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:auth": "playwright test tests/auth/",
    "test:admin": "playwright test tests/admin/",
    "test:crud": "playwright test tests/crud/",
    "test:performance": "playwright test tests/performance/",
    "test:visual": "playwright test tests/visual/",
    "test:ci": "CI=true playwright test",
    "report": "playwright show-report",
    "codegen": "playwright codegen",
    "install-browsers": "playwright install"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/node": "^20.10.0",
    "dotenv": "^16.3.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "csv-parse": "^5.5.2",
    "pdf-parse": "^1.1.1"
