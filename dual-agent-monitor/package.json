{
  "name": "dual-agent-monitor",
  "type": "module",
  "version": "2.1.0",
  "description": "Web UI for monitoring dual-agent Claude Code system",
  "main": "server/websocket-server.js",
  "scripts": {
    "dev": "pnpm concurrently \"pnpm run server:dev\" \"pnpm run client:dev\"",
    "build": "pnpm run server:build && pnpm run client:build",
    "build:validate": "pnpm run typecheck && pnpm run lint && pnpm run build",
    "start": "node dist/server/websocket-server.js",
    "server:dev": "pnpm tsx server/websocket-server.ts",
    "server:build": "tsc --project server/tsconfig.json",
    "client:dev": "pnpm vite",
    "client:build": "pnpm vite build",
    "client:preview": "pnpm vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:performance": "k6 run tests/performance/load-test.js",
    "test:security": "pnpm audit && pnpm run test:security:dependencies",
    "test:security:dependencies": "better-npm-audit audit --level moderate",
    "test:all": "pnpm run test:unit && pnpm run test:integration && pnpm run test:e2e",
    "lint": "eslint src server --ext .ts,.tsx",
    "lint:fix": "eslint src server --ext .ts,.tsx --fix",
    "prettier": "prettier --check src server",
    "prettier:fix": "prettier --write src server",
    "typecheck": "tsc --noEmit && tsc --project server/tsconfig.json --noEmit",
    "pre-commit": "pnpm run lint && pnpm run prettier && pnpm run typecheck",
    "db:migrate": "pnpm tsx server/database/migrate.ts",
    "db:seed": "pnpm tsx server/database/seed.ts",
    "db:reset": "pnpm tsx server/database/reset.ts",
    "db:backup": "pnpm tsx server/database/backup.ts",
    "db:restore": "pnpm tsx server/database/restore.ts",
    "db:test:setup": "echo 'Setting up test database...'",
    "docker:build": "docker build -t dual-agent-monitor .",
    "docker:build:api": "docker build -f Dockerfile.api -t dual-agent-monitor-api .",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:api": "docker-compose -f docker-compose.api.yml --env-file .env.api up -d",
    "docker:api:build": "docker-compose -f docker-compose.api.yml --env-file .env.api up --build -d",
    "docker:api:logs": "docker-compose -f docker-compose.api.yml logs -f",
    "docker:api:stop": "docker-compose -f docker-compose.api.yml down",
    "docker:api:restart": "docker-compose -f docker-compose.api.yml restart api-server",
    "docker:api:status": "docker-compose -f docker-compose.api.yml ps",
    "api:deploy": "bash scripts/deploy-api.sh",
    "api:deploy:win": "powershell -ExecutionPolicy Bypass -File scripts/deploy-api.ps1",
    "api:test": "node scripts/test-api.js",
    "fullstack:deploy": "bash scripts/deploy-full-stack.sh",
    "fullstack:deploy:win": "powershell -ExecutionPolicy Bypass -File scripts/deploy-full-stack.ps1",
    "fullstack:stop": "docker-compose -f docker-compose.full-stack.yml down",
    "fullstack:restart": "docker-compose -f docker-compose.full-stack.yml restart",
    "fullstack:logs": "docker-compose -f docker-compose.full-stack.yml logs -f",
    "fullstack:status": "docker-compose -f docker-compose.full-stack.yml ps",
    "fullstack:clean": "docker-compose -f docker-compose.full-stack.yml down --rmi all --volumes --remove-orphans",
    "fullstack:test": "bash scripts/test-networking.sh",
    "fullstack:health": "docker-compose -f docker-compose.full-stack.yml --profile test run --rm healthcheck",
    "deploy:staging": "pnpm run build:validate && pnpm run docker:build",
    "deploy:production": "pnpm run test:all && pnpm run build:validate && pnpm run docker:build",
    "deploy:fullstack": "pnpm run fullstack:deploy"
  },
  "dependencies": {
    "@reactflow/core": "^11.11.4",
    "@reactflow/node-resizer": "^2.2.14",
    "@reactflow/node-toolbar": "^1.3.14",
    "@types/cors": "^2.8.14",
    "axios": "^1.7.4",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.18",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.3",
    "@types/ws": "^8.5.5",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "chart.js": "^4.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "d3": "^7.9.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "react-chartjs-2": "^5.3.0",
    "react-flow-renderer": "^10.3.17",
    "react-vis-network-graph": "^3.0.1",
    "reactflow": "^11.11.4",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0",
    "vis-network": "^10.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@playwright/test": "^1.42.1",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-context-menu": "^2.1.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^5.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.15",
    "better-npm-audit": "^3.7.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "concurrently": "^8.2.1",
    "date-fns": "^2.30.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "framer-motion": "^10.16.4",
    "happy-dom": "^13.6.2",
    "husky": "^9.0.11",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "lucide-react": "^0.277.0",
    "msw": "^2.2.1",
    "postcss": "^8.4.29",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-resizable-panels": "^0.0.55",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0",
    "react-virtualized": "^9.22.5",
    "recharts": "^2.8.0",
    "sonner": "^1.0.3",
    "supertest": "^6.3.4",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2",
    "use-debounce": "^10.0.0",
    "vite": "^4.4.9",
    "vitest": "^1.3.1",
    "ws": "^8.16.0",
    "zustand": "^4.4.1"
  }
}
