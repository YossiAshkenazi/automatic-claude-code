{
  "name": "dual-agent-monitor",
  "version": "1.0.0",
  "description": "Web UI for monitoring dual-agent Claude Code system",
  "main": "server/index.js",
  "scripts": {
    "dev": "node node_modules/concurrently/dist/bin/concurrently.js \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build && npm run server:build",
    "start": "node dist/server/index.js",
    "server:dev": "node node_modules/tsx/dist/cli.mjs server/index.ts",
    "server:build": "tsc --project server/tsconfig.json",
    "client:dev": "node node_modules/vite/bin/vite.js",
    "client:build": "node node_modules/vite/bin/vite.js build",
    "client:preview": "node node_modules/vite/bin/vite.js preview"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2",
    "uuid": "^9.0.0",
    "@types/express": "^4.17.18",
    "@types/cors": "^2.8.14",
    "@types/ws": "^8.5.5",
    "@types/uuid": "^9.0.3"
  },
  "devDependencies": {
    "vite": "^4.4.9",
    "@vitejs/plugin-react": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "typescript": "^5.2.2",
    "tsx": "^3.12.10",
    "concurrently": "^8.2.1",
    "@types/node": "^20.5.9",
    "tailwindcss": "^3.3.3",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "lucide-react": "^0.277.0",
    "react-syntax-highlighter": "^15.5.0",
    "@types/react-syntax-highlighter": "^15.5.7",
    "date-fns": "^2.30.0",
    "recharts": "^2.8.0"
  }
}