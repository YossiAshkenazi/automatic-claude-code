{
  "name": "dual-agent-monitor",
  "version": "1.0.0",
  "description": "Web UI for monitoring dual-agent Claude Code system",
  "main": "server/index.js",
  "scripts": {
    "dev": "node node_modules/concurrently/dist/bin/concurrently.js \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run server:build && npm run client:build",
    "start": "node dist/server/index.js",
    "server:dev": "node node_modules/tsx/dist/cli.mjs server/index.ts",
    "server:build": "tsc --project server/tsconfig.json",
    "client:dev": "node node_modules/vite/bin/vite.js",
    "client:build": "node node_modules/vite/bin/vite.js build",
    "client:preview": "node node_modules/vite/bin/vite.js preview",
    "db:migrate": "node node_modules/tsx/dist/cli.mjs server/database/migrate.ts",
    "db:seed": "node node_modules/tsx/dist/cli.mjs server/database/seed.ts",
    "db:reset": "node node_modules/tsx/dist/cli.mjs server/database/reset.ts",
    "db:backup": "node node_modules/tsx/dist/cli.mjs server/database/backup.ts",
    "db:restore": "node node_modules/tsx/dist/cli.mjs server/database/restore.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.3",
    "@types/ws": "^8.5.5",
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-context-menu": "^2.1.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^5.0.0",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "concurrently": "^8.2.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.277.0",
    "postcss": "^8.4.29",
